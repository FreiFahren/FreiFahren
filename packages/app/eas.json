{
  "cli": {
    "version": ">= 10.1.1",
    "appVersionSource": "local",
  },
  "build": {
    "default": {},
    "development": {
      "extends": "default",
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "extends": "default",
      "distribution": "internal"
    },
    "internal": {
      "extends": "default"
    },
    "alpha": {
      "extends": "default"
    },
    "production": {
      "extends": "default",
    }
  },
  "submit": {
    "default": {},
    "internal": {
      "extends": "default",
      "android": {
        "track": "internal"
      }
    },
    "alpha": {
      "extends": "default",
      "android": {
        "track": "alpha"
      }
    },
    "production": {
      "extends": "default",
      "android": {
        "track": "production"
      }
    }
  }
}
